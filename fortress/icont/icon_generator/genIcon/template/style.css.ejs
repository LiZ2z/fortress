.<%= prefix %>-icon {
  display: inline-block;
  color: inherit;
  text-align: center;
  line-height: 1em;
  width: 1em;
  height: 1em;
  vertical-align: middle;
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
  overflow: hidden;
}

.<%= prefix %>-icon > svg {
  width: 1em;
  height: 1em;
}

.<%= prefix %>-icon * {
  fill: currentColor !important;
}


.icont-placeholder-loading {
  animation: icont-placeholder-loading-rotate linear 0.6s infinite;
}

@keyframes icont-placeholder-loading-rotate {
  from {
    transform: rotateZ(0);
  }
  to {
    transform: rotateZ(360deg);
  }
}
